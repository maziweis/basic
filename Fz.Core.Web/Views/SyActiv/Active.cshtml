
@{
    Layout = "~/Views/Shared/_LayoutTopLeft.cshtml";
}

<div class="sysAct schoolInfo">
    <div class="moduleTitle">
        <h3 class="tc">在线激活</h3>
    </div>
    <form>
        <div class="infoTable">
            <div class="form-group clearfix">
                <label class="col-md-2 control-label" for="Name">请输入激活码：</label>
                <div class="col-md-5"><input autocomplete="off" class="form-control" data-val="true" data-val-maxlength="" data-val-maxlength-max="50" data-val-required="" id="Name" name="Name" type="text" value=""></div>
                <div class="col-md-5"><span class="field-validation-valid" data-valmsg-for="Name" data-valmsg-replace="true"></span></div>
            </div>
            <div class="form-group clearfix">
                <label class="col-md-2 control-label" for="Code">请输入学校名称：</label>
                <div class="col-md-5"><input autocomplete="off" class="form-control" data-val="true" data-val-maxlength="" data-val-maxlength-max="100" id="Code" name="Code" type="text" value=""></div>
                <div class="col-md-5"><span class="field-validation-valid" data-valmsg-for="Code" data-valmsg-replace="true"></span></div>
            </div>
            <div class="form-group clearfix">
                <label class="col-md-2 control-label" for="Principal">请输入手机号码：</label>
                <div class="col-md-5"><input autocomplete="off" class="form-control" data-val="true" data-val-maxlength="" data-val-maxlength-max="15" id="Principal" name="Principal" type="tel" value=""></div>
                <div class="col-md-5"><span class="field-validation-valid" data-valmsg-for="Principal" data-valmsg-replace="true"></span></div>
            </div>
            <div class="buttonBox"><a class="btn btn-default" href="">下一步</a><button class="btn btn-default">取消</button></div>
        </div>
    </form>
</div>
