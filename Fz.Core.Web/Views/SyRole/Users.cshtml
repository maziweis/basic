@{
    Layout = null;
    ViewBag.IsEnableds = Fz.Common.Dict.IsEnabled.Get();
    ViewBag.Roles = Fz.Core.Bll.SyRoleBll.GetDictManager();
    Dictionary<int, string> Roles = ViewBag.Roles as Dictionary<int, string>;
}

@model Fz.Core.VModel.SyRole.RUser

@using (Ajax.BeginForm("Users", "SyRole", new AjaxOptions { UpdateTargetId = "datas", InsertionMode = InsertionMode.Replace }, new { @id = "form2", enctype = "multipart/form-data" }))
{
    @Html.HiddenFor(m => m.Grid.Pager.PageIndex)
    @Html.HiddenFor(m => m.Grid.Pager.PageSize)
}
<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
    <h4 class="modal-title">查看用户</h4>
</div>
<div id="datas" class="modal-body">

    @Html.Partial("_UsersGrid", Model)

</div>